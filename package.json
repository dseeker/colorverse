{
  "name": "colorverse",
  "version": "1.0.0",
  "description": "Free AI-Generated Coloring Pages",
  "type": "module",
  "main": "app.js",
  "scripts": {
    "start": "serve",
    "dev": "node app.js",
    "generate": "node generate-image.js",
    "generate:dev": "nodemon generate-image.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest src/tests/unit",
    "test:integration": "vitest src/tests/integration",
    "test:e2e": "vitest src/tests/e2e",
    "test:e2e:playwright": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:image": "node generate-image.js --test",
    "test:metadata": "node generate-image.js --test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,ts,jsx,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,jsx,tsx,json,css,md}\"",
    "prepare": "husky",
    "cleanup": "node generate-image.js --cleanup",
    "install:deps": "npm install",
    "test:original": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "ai",
    "coloring",
    "pages",
    "pollinations"
  ],
  "author": "dseeker",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.12.2",
    "dotenv": "^16.4.5",
    "exiftool-vendored": "^16.0.0",
    "sharp": "^0.33.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^17.0.0",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "nodemon": "^3.0.0",
    "prettier": "^3.7.4",
    "vitest": "^4.0.16"
  }
}